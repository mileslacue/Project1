package com.revature.models;

import java.sql.Timestamp;

//POJO representing the reimbursement table
public class Reimbursment {
	
	private int reimbID;
	private double amount;
	private Timestamp submitted;
	private Timestamp approved;
	//private /*data type for reciept*/     description;
	private Employee author;
	private Employee resolver;
	private int statusID;
	private int typeID;
	
	public Reimbursment() {}
	
	public Reimbursement(int id, double amt, Timestamp submit, Employee aut, Employee resolve, int stat, int type) {
	
		this.reimbID = id;
		this.amount = amt;
		this.submitted = submit;
		this.author = aut;
		this.resolver = resolve;
		this.statusID = stat;
		this.typeID = type;
	}
	
	public Reimbursement(int id, double amt, /*data type*/ submit, /*data type*/ approve, /*data type*/ describe, Employee aut, Employee resolve, int stat, int type) {
	
		
	}
}
