package com.revature.models;

import java.sql.Timestamp;

//POJO representing the reimbursement table
public class Reimbursment {
	
	private int reimbID;
	private double amount;
	private Timestamp submitted;
	private Timestamp approved;
	//private     /*data type for reciept*/     description;
	private Employee author;
	private Employee resolver;
	private int statusID;
	private int typeID;
	
	public Reimbursment() {}
	
	public Reimbursment(int id, double amt, Timestamp submit, Employee author, Employee resolve, int stat, int type) {
	
		this.reimbID = id;
		this.amount = amt;
		this.submitted = submit;
		this.author = author;
		this.resolver = resolve;
		this.statusID = stat;
		this.typeID = type;
	}
	
	public Reimbursment(int id, double amt, Timestamp submit, Timestamp approve, Employee author, Employee resolve, int stat, int type) {
		this.reimbID = id;
		this.amount = amt;
		this.submitted = submit;
		this.author = author;
		this.approved = approve;
		this.resolver = resolve;
		this.statusID = stat;
		this.typeID = type;
	}
	
	
	
	
}
